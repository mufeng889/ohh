{
  "name": "ohh",
  "type": "module",
  "packageManager": "pnpm@9.15.1",
  "homepage": "https://github.com/mufeng889/ohh",
  "bugs": "https://github.com/mufeng889/ohh/issues",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=8192 turbo build",
    "build:cli": "pnpm run build --filter=@ohh-889/cli",
    "build:eslint": "pnpm run build --filter=@ohh-889/eslint-config",
    "build:watch": "unbuild --stub",
    "cleanup": "ohh cleanup",
    "commit": "ohh git-commit",
    "commit:zh": "ohh git-commit -l=zh-cn",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "publish-pkg": "pnpm -r publish --access public --no-git-checks  --registry https://registry.npmjs.org/ --filter='@ohh-889/commit-config'",
    "release": "ohh release",
    "update-pkg": "ohh update-pkg"
  },
  "devDependencies": {
    "@ohh-889/cli": "workspace:*",
    "@ohh-889/eslint-config": "workspace:*",
    "@types/eslint-config-prettier": "6.11.3",
    "@types/node": "22.10.2",
    "@types/react": "19.0.2",
    "@types/react-dom": "19.0.2",
    "@unocss/core": "0.65.2",
    "@unocss/eslint-config": "0.65.2",
    "@unocss/preset-uno": "0.65.2",
    "cross-env": "7.0.3",
    "eslint": "9.17.0",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-react-refresh": "0.4.16",
    "eslint-plugin-sort": "4.0.0",
    "eslint-plugin-vue": "9.32.0",
    "prettier": "3.4.2",
    "prettier-plugin-jsdoc": "1.3.0",
    "prettier-plugin-json-sort": "0.0.2",
    "tsup": "8.3.5",
    "tsx": "4.19.2",
    "turbo": "2.3.3",
    "typescript": "5.7.2",
    "unbuild": "3.0.1",
    "vite": "6.0.5",
    "vue-eslint-parser": "9.4.3"
  }
}
